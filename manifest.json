{
	"name": "RaidRedirector",
	"author": "VacodaC, Vacoda, VaccyC, FileGeist",
	"version": "0.1.6",
	"description": "Redirects without a '?referrer=raid'",
	"content_security_policy": "default-src 'self'",
	"permissions": [
		"webRequest",
		"*://twitch.tv/*", 
		"*://www.twitch.tv/*",
		"webRequestBlocking",
		"webNavigation",
		"tabs"
	],
	"background": {"scripts":["redir.js"]},
	"icons": {
		"16": "images/R16.png",
		"32": "images/R32.png",
		"48": "images/R48.png",
		"128": "images/R128.png"
	},
	"manifest_version": 2
}
